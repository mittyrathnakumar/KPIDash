<div class="row col-md-10">		
	<table class="table-bordered table-condensed" id="dataTable">
		<thead>
			<tr>					
				<th>No.</th>
				<th>Projectname</th>
				<th>P1 & P2 Defects</th>				
			</tr>
		</thead>
		<tbody>
			
			{% if KPIResults|length > 0 %}
				{% for month, resultArr in KPIResults %}
					<tr><td class="text-primary" colspan="8"><strong>{{ month }}</strong></td></tr>
					{% if resultArr|length > 0 %}
						{% for row in resultArr %}
							<tr>
							<td>{{ loop.index }}</td>	
							<td>{{ row['PROJECTNAME'] }}</td>
							<td>{{ row['P1_P2'] }}</td>							
							</tr>
						{% endfor %}						
					{% else %}
						<tr><td class="text-primary div-center" colspan="8">N/A</td></tr>
					{% endif %}
		    	{% endfor %}
		    {% else %}
		    	<tr><td class="text-primary div-center" colspan="8">No data available !!!</td></tr>
		    {% endif %}
		</tbody>
	</table>			
</div>