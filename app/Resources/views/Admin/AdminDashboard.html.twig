{% extends 'layout.html.twig' %}

{% block breadcrumb %}
	<div class="row">
		<ol class="breadcrumb">
			<li><a href="#">Admin</a></li>
			<li class="active">Admin Dashboard</li>
		</ol>
	</div>
{% endblock %}

{% block content %}
	<h1 class="page-header">Latest TDM Requests</h1>
	<div class="row">	
		<div class="">TDM Request Status
		<div id="tdm_requeststatus"></div>
		</div>
		<div class="">TDM Requests by Yearly Quarter
		<div id="tdm_yearlystatus"></div>
		</div>
	</div>
	<hr>
	
	<div class="row">
		<ol class="list-group">
			<li class="list-group-item"><a href="{{ path('CreateUser') }}">Create a new User</a></li>
			<li class="list-group-item"><a href="#">Modify Existing User</a></li>
		</ol>
	</div>
{% endblock %}

{% block documentready %}	
	<script>		
		init();
	</script>
{% endblock %}

{% block javascripts %}
	{{ parent() }}
	<script type="text/javascript" src="https://www.gstatic.com/charts/loader.js"></script>
	<script src="{{ asset('js/Admin/AdminDashboard.js') }}"></script>	
{% endblock %}


